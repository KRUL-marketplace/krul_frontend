{
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": true,
  "arrowParens": "avoid",
  "importOrder": [
    "react",
    "react-router-dom",
    "next",
    "app",
    "@chakra-ui",
    "client",
    "server",
    "utils",
    "<THIRD_PARTY_MODULES>",
    "[./]",
    "[../]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "overrides": [
    {
      "files": [
        "*.json",
        "*.html"
      ]
    }
  ]
}
